  object QBesKesinti: TUniQuery
    DataTypeMap = <>
    Connection = FData.UniMemur
    SQL.Strings = (
      'SELECT  Yil ,'
      '        Ay ,'
      '        Donem ,'
      '        Sicno ,'
      '        Dair ,'
      '        SskNo ,'
      '        ISNULL(Adi, '#39#39') + '#39' '#39' + ISNULL(Soyadi, '#39#39') AdiSoyadi ,'
      '        Pkaz41 AS BesKesinti'
      'FROM    dbo.Phesap'
      'WHERE   Ay = :Ay'
      '        AND Sicno BETWEEN 1 AND 9999'
      '        AND Pkaz41 > 0'
      'ORDER BY Adi, Soyadi')
    Active = True
    Left = 216
    Top = 144
    ParamData = <
      item
        DataType = ftString
        Name = 'Ay'
        Value = '01'
      end>
    object QBesKesintiYil: TWideStringField
      FieldName = 'Yil'
      Required = True
      Size = 4
    end
    object QBesKesintiAy: TWideStringField
      FieldName = 'Ay'
      Required = True
      Size = 2
    end
    object QBesKesintiDonem: TWideStringField
      FieldName = 'Donem'
      Required = True
      Size = 50
    end
    object QBesKesintiSicno: TIntegerField
      FieldName = 'Sicno'
      Required = True
    end
    object QBesKesintiDair: TWideStringField
      FieldName = 'Dair'
      Size = 50
    end
    object QBesKesintiSskNo: TWideStringField
      FieldName = 'SskNo'
      Size = 25
    end
    object QBesKesintiAdiSoyadi: TWideStringField
      FieldName = 'AdiSoyadi'
      ReadOnly = True
      Required = True
      Size = 101
    end
    object QBesKesintiBesKesinti: TFloatField
      FieldName = 'BesKesinti'
      DisplayFormat = '#,##0.00'
    end
  end
