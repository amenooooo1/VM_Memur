  object QDonem: TUniQuery
    DataTypeMap = <>
    Connection = FData.UniMemur
    SQL.Strings = (
      'DECLARE @Ay CHAR(2) = :Ay;'
      ''
      'SELECT d.Ay,'
      '       d.Donem,'
      '       ROUND(k.VergiMuaf * k.Damor, 2) dvIstisna'
      'FROM dbo.Donemler d'
      
        '    INNER JOIN dbo.Katsayilar k ON k.Yil = d.Yil AND k.Ay = d.Ay' +
        ' AND k.Donem = d.Donem'
      'WHERE d.Ay = @Ay;')
    AfterOpen = QDonemAfterOpen
    Left = 312
    Top = 152
    ParamData = <
      item
        DataType = ftString
        Name = 'Ay'
      end>
    object QDonemAy: TWideStringField
      FieldName = 'Ay'
      Required = True
      Size = 2
    end
    object QDonemDonem: TWideStringField
      FieldName = 'Donem'
      Required = True
      Size = 50
    end
    object QDonemdvIstisna: TFloatField
      FieldName = 'dvIstisna'
      ReadOnly = True
    end
  end
  object dDonem: TUniDataSource
    DataSet = QDonem
    Left = 312
    Top = 208
  end
  object QHesap: TUniQuery
    DataTypeMap = <>
    Connection = FData.UniMemur
    SQL.Strings = (
      'SELECT Sicno,'
      '       Adi,'
      '       Soyadi,'
      '       Donem,'
      '       Dair,'
      '       GelirMat,'
      '       GelirVer,'
      '       GelirVergisiMatrah,'
      '       GelirVergisiMahsup,'
      '       DamMat,'
      '       DamVer,'
      '       DamgaVergisiMahsup'
      'FROM dbo.Phesap'
      'WHERE Ay = :Ay'
      'AND Donem = :Donem;')
    AfterOpen = QHesapAfterOpen
    Left = 440
    Top = 160
    ParamData = <
      item
        DataType = ftString
        Name = 'Ay'
      end
      item
        DataType = ftString
        Name = 'Donem'
      end>
