  object QBesKontrol: TUniQuery
    DataTypeMap = <>
    Connection = FData.UniMemur
    SQL.Strings = (
      'DECLARE @Yil CHAR(4) = :Yil ,'
      '  @Ay CHAR(2) = :Ay ,'
      '  @Donem NVARCHAR(50) = :Donem'
      ''
      'SELECT  Sicno ,'
      '        Donem ,'
      '        Adi ,'
      '        Soyadi ,'
      '        ( DATEDIFF(YEAR, DogumTarihi, GETDATE())'
      
        '          - CASE WHEN DATEADD(YY, DATEDIFF(YEAR, DogumTarihi, GE' +
        'TDATE()),'
      
        '                              DogumTarihi) > GETDATE() THEN 1 EL' +
        'SE 0 END ) AS Yas ,'
      '        ISNULL(BesKesinti, 0) AS BesKesinti'
      'INTO    #BesKesinti'
      'FROM    dbo.Sicil'
      'WHERE   Yil = @Yil AND Ay = @AY AND Donem = @Donem'
      ''
      
        '--IF ( SELECT COUNT(*) FROM #BesKesinti WHERE  Yas >= 45 AND Bes' +
        'Kesinti = 1) = 0'
      '  UPDATE  dbo.Sicil'
      '  SET     BesKesinti = 0'
      '  FROM    dbo.Sicil s'
      '          INNER JOIN ( SELECT Sicno ,'
      '                              Donem ,'
      '                              Adi ,'
      '                              Soyadi'
      '                       FROM   #BesKesinti'
      
        '                       WHERE  Yas >= 45 AND BesKesinti = 1 ) AS ' +
        'b ON b.Sicno = s.Sicno'
      '                               AND b.Donem = s.Donem'
      ''
      'SELECT  Sicno ,'
      '        Adi + '#39' '#39' + Soyadi AS Personel'
      'FROM    #BesKesinti'
      'WHERE   Yas >= 45'
      '        AND BesKesinti = 1'
      ''
      'DROP TABLE #BesKesinti')
    Left = 88
    Top = 504
    ParamData = <
      item
        DataType = ftString
        Name = 'Yil'
      end
      item
        DataType = ftString
        Name = 'Ay'
      end
      item
        DataType = ftString
        Name = 'Donem'
      end>
    object QBesKontrolSicno: TIntegerField
      FieldName = 'Sicno'
      Required = True
    end
    object QBesKontrolPersonel: TWideStringField
      FieldName = 'Personel'
      ReadOnly = True
      Size = 101
    end
  end
  object DBesKontrol: TUniDataSource
    DataSet = QBesKontrol
    Left = 144
    Top = 520
  end
