  object UniQuery2: TUniQuery
    DataTypeMap = <>
    Connection = FData.UniMemur
    SQL.Strings = (
      
        'SELECT D.Pkaz26, D.Pkaz27, D.Pkaz28, D.Pkaz29, D.Pkaz30, D.Pkaz3' +
        '1, D.Pkaz32, D.Pkaz33, D.Pkaz34,D.Pkaz35,D.Pkaz36,D.Pkaz37,D.Pka' +
        'z38,D.Pkaz39,D.Pkaz40, D.SskIs, D.SskIsv, D.GelirVer,  D.DamVer,' +
        ' D.Net,D.GenSagKisi,D.GenSagKurum,D.ISMYS,D.ISVMYS,D.ISGSS,D.ISV' +
        'GSS,D.A3,D3.Eucret,D3.Once5510,D3.Kisi,D2.KOD'
      'FROM "Phesap" D,"Sicil" D3,"TSendika" D2'
      'WHERE (D3.yil= D.Yil) and (D3.ay= D.ay) and'
      
        '(D3.Donem= D.donem) and (D3.Sicno= D.Sicno) and (D3.Sendika= D2.' +
        'id)'
      'and (D.Yil = :Yil )  AND ( D.Ay =:Ay )  AND'
      '( D.Donem =:Donem ) AND (D.Dair =:t1) '
      'ORDER BY  D.Dair, D.Sicno, D.Yil, D.Ay')
    Left = 506
    Top = 231
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Yil'
      end
      item
        DataType = ftUnknown
        Name = 'Ay'
      end
      item
        DataType = ftUnknown
        Name = 'Donem'
      end
      item
        DataType = ftUnknown
        Name = 't1'
      end>
  end
