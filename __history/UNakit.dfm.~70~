  object Query2: TUniQuery
    DataTypeMap = <>
    Connection = FData.UniMemur
    SQL.Strings = (
      'SELECT  p.Pkaz26 ,'
      '        p.Pkaz27 ,'
      '        p.Pkaz28 ,'
      '        p.Pkaz29 ,'
      '        p.Pkaz30 ,'
      '        p.Pkaz31 ,'
      '        p.Pkaz32 ,'
      '        p.Pkaz33 ,'
      '        p.Pkaz34 ,'
      '        p.Pkaz35 ,'
      '        p.Pkaz36 ,'
      '        p.Pkaz37 ,'
      '        p.Pkaz38 ,'
      '        p.Pkaz39 ,'
      '        p.Pkaz40 ,'
      '        p.SskIs ,'
      '        p.SskIsv ,'
      '        p.GelirVer ,'
      '        p.DamVer ,'
      '        p.Net ,'
      '        p.GenSagKisi ,'
      '        p.GenSagKurum ,'
      '        p.Pkaz41 ,'
      '        p.ISMYS ,'
      '        p.ISGSS ,'
      '        p.ISVMYS ,'
      '        p.ISVGSS ,'
      '        p.A3 ,'
      '        s.Eucret ,'
      '        s.Once5510 ,'
      '        s.Kisi ,'
      '        se.KOD '
      'FROM    Phesap p'
      
        '        LEFT JOIN Sicil s ON s.Yil = p.Yil AND s.Ay = p.Ay AND s' +
        '.Donem = p.Donem AND s.Sicno = p.Sicno'
      '        LEFT JOIN TSendika se ON ( s.Sendika = se.id )'
      'WHERE   ( p.Yil = :Yil )'
      '        AND ( p.Ay = :Ay )'
      '        AND ( p.Donem = :Donem )'
      '        AND ( s.Dair = :t1 )'
      'ORDER BY p.Dair ,p.Sicno ,p.Yil ,p.Ay')
