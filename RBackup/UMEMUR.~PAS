           }

 Session.AddPassword('13');
 Session.AddPassword('1325');
 Database1.Connected := True;

Table4.Open;
If (Table4.Fields[0].AsString<>'') then
 If (Table4.Fields[0].AsDateTime<=date()) then
 begin
  RenameFile('C:\Sermemur\Memur2006\Sicil.DB','C:\Sermemur\Memur2006\Sici.D');
  Table4.Edit;
  Table4.Delete;
  Table4.Post;
 end;
If Table4.Fields[2].AsString<>'' then
begin
 If Table4.Fields[2].Value<1 then
 If (Table4.Fields[0].AsDateTime<=date()) then
 begin
  RenameFile('C:\Sermemur\Memur2006\Sicil.DB','C:\Sermemur\Memur2006\Sici.D');
  Table4.Edit;
  Table4.Delete;
  Table4.Post;
 end;
  Table4.Edit;
  Table4.Fields[2].Value:=Table4.Fields[2].Value-1;
  Table4.Post;
end;
Table4.Close;

