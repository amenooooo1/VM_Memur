procedure TDMEMUR.TSICILAfterPost(DataSet: TDataSet);
begin
DBISaveChanges((dataSet as TBDEDataSet).Handle);
end;





procedure TDMEMUR.TDaireler1AfterEdit(DataSet: TDataSet);
