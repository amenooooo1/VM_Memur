TDAIR.Edit;
end;

procedure TDMEMUR.TSicil1AfterEdit(DataSet: TDataSet);
begin
TSICIL.Edit;
end;

procedure TDMEMUR.TSICILBeforeDelete(DataSet: TDataSet);
begin
if TSicil1.RecordCount > 0 then TSicil1.Delete;

end;

