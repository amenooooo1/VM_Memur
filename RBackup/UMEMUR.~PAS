procedure TDMEMUR.DISCIVCreate(Sender: TObject);
var dbnet:String;
begin
{if not Table1.Exists then
 begin
  Table1.TableType := ttParadox;
  Table1.TableName := 'AylikEmliste';
      with Table1.FieldDefs do begin
      Clear;
      with AddFieldDef do begin
        Name := 'Sicno';
        DataType := ftFloat;
        Required := False;
      end;

      with AddFieldDef do begin
        Name := 'Adsoy';
        DataType := ftString;
        Size := 50;
       end;
      with AddFieldDef do begin
        Name := 'Sskv';
        DataType := ftFloat;
       end;
      with AddFieldDef do begin
        Name := 'SskNO';
        DataType := ftString;
        Size := 15;
       end;

      with AddFieldDef do begin
        Name := 'Sski';
        DataType := ftFloat;
       end;
      with AddFieldDef do begin
        Name := 'art';
        DataType := ftFloat;
       end;
      with AddFieldDef do begin
        Name := 'Sskver';
        DataType := ftFloat;
       end;

      with AddFieldDef do begin
        Name := 'Gec';
        DataType := ftFloat;
       end;

    end;
   with Table1.IndexDefs do begin
      Clear;
      with AddIndexDef do begin
        Name := '';
        Fields := 'Sicno';
        Options := [ixPrimary];
      end;
    end;
    Table1.CreateTable;

  end; }
{if not TYAZ.Exists then
 begin
  TYAZ.TableType := ttParadox;
  TYAZ.TableName := 'YAZ';
      with TYAZ.FieldDefs do begin
      Clear;

      with AddFieldDef do begin
        Name := 'NOO';
        DataType := ftInteger;
  //      Size := 50;
       end;
      with AddFieldDef do begin
        Name := 'NO1';
        DataType := ftInteger;
  //      Size := 50;
       end;

  {    with AddFieldDef do begin
        Name := 'Tahakkuk';
        DataType := ftCurrency;
       end;

      with AddFieldDef do begin
        Name := 'Tahsilat';
        DataType := ftCurrency;
       end;
    end;
   with Table3.IndexDefs do begin
      Clear;
      with AddIndexDef do begin
        Name := '';
        Fields := 'Gelirkod';
        Options := [ixPrimary];
      end;
    end;
    TYAZ.CreateTable;

  end;





if not Table4.Exists then
 begin
  Table4.TableType := ttParadox;
  Table4.TableName := 'Abooo';
      with Table4.FieldDefs do begin
      Clear;
      with AddFieldDef do begin
        Name := 'Tar';
        DataType := ftDate;
        Required := False;
      end;

